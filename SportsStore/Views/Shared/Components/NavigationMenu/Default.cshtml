@model IEnumerable<string>

<a class="btn btn-toolbar btn-outline-secondary justify-content-center" asp-action="Index" asp-controller="Home" asp-route-category="">
    Home
</a>

@foreach (string category in Model) {
    <a class="btn btn-toolbar btn-outline-secondary justify-content-center" asp-action="Index" asp-controller="Home" asp-route-category="@category" asp-route-productPage="1">
        @category
    </a>
}